;\n  };\n\n  const values = { firstName, lastName, email, password };\n\n  switch (step) {\n    case 1:\n      return (\n        <SignUp\n          nextStep={nextStep}\n          handleChange={handleChange}\n          values={values}\n        />\n      );\n    case 2:\n      return (\n        <PictureProfile\n          nextStep={nextStep}\n          prevStep={prevStep}\n          handleChange={handleChange}\n          values={values}\n        />\n      );\n    case 3:\n      return <Welcome />;\n  }\n}\n","/Volumes/chia xiong/Git/Sofofiling/client/src/components/MultiStepForm/FormDetail/Welcome.js",["68"],"import React from \"react\";\nimport Container from \"@material-ui/core/Container\";\nimport Typography from \"@material-ui/core/Typography\";\nimport { makeStyles } from \"@material-ui/core/styles\";\n\nconst useStyles = makeStyles(theme => ({\n  wrapper: {\n    backgroundColor: \"#F5AB7C\",\n    position: \"absolute\",\n    top: \"0\",\n    bottom: \"0\",\n    right: \"0\",\n    left: \"0\",\n  },\n  form: {\n    backgroundColor: \"#fff\",\n    margin: theme.spacing(15, 50, 8, 50),\n    display: \"flex\",\n    flexDirection: \"column\",\n    alignItems: \"center\",\n    justifyContent: \"center\",\n    borderRadius: \"50px\",\n    minWidth: \"500px\",\n    minHeight: \"600px\",\n  },\n  message: {\n    padding: theme.spacing(8, 20, 4, 20),\n    color: \"#827F7F\",\n    fontSize: \"2em\",\n    fontWeight: \"bold\",\n  },\n}));\n\nexport default function Welcome() {\n  const classes = useStyles();\n  return (\n    <div className={classes.wrapper}>\n      <div className={classes.form}>\n        <Typography variant=\"h5\" className={classes.message}>\n          Welcome to the Sofofiling family!\n          <br />\n          <br />\n          Our name comes from the word sophophiles, a person who loves to gather\n          knowledge! Start today to learn, or share what you know to others!\n        </Typography>\n      </div>\n    </div>\n  );\n}\n","/Volumes/chia xion